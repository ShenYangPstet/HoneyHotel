<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.photonstudio.mapper.StoreroomMapper">
	<select id="findObject" resultType="Storeroom">
	select * from `${dBname}`.storeroom 
	<if test="pageSize!=null">
	limit #{startIndex},#{pageSize}
    </if>
	</select>
	<select id="getRowCount" resultType="int">
	select count(*) from `${dBname}`.storeroom 
	</select>
	<insert id="insertObject">
	insert into `${dBname}`.storeroom (roomname,storeroomExplain) values 
	(#{storeroom.roomname},#{storeroom.storeroomExplain})
	</insert>
	<delete id="deleteObjectById">
	delete from `${dBname}`.storeroom
	<where>
	id in 
		 <foreach collection="ids" open="("
                close=")"
                separator=","
                item="id">
                #{id}
         </foreach> 
	</where>
	</delete>
	<update id="updateObject">
	update `${dBname}`.storeroom 
	<set>
	<if test="storeroom.roomname!=null and storeroom.roomname!=''">
	roomname=#{storeroom.roomname},
	</if>
	<if test="storeroom.storeroomExplain!=null and storeroom.storeroomExplain!=''">
	storeroomExplain=#{storeroom.storeroomExplain},
	</if>
	id=#{storeroom.id}
	</set>
	<where>
	id=#{storeroom.id}
	</where>
	</update>
</mapper>