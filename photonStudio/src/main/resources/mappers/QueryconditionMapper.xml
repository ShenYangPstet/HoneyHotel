<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.photonstudio.mapper.QueryconditionMapper">
	<select id="findObject" resultType="Querycondition">
		select * from `${dBname}`.querycondition
	</select>
	<insert id="insertObject">
		insert into `${dBname}`.querycondition (conditionName) values
		(#{querycondition.conditionName})

	</insert>
	<delete id="deleteObjectById">
		delete from `${dBname}`.querycondition
		<where>
			conditionId=#{id}
		</where>
	</delete>
	<update id="updateObject">
		update `${dBname}`.querycondition
		<set>
			conditionName=#{querycondition.conditionName}
		</set>
		<where>
		conditionId=#{querycondition.conditionId}
		</where>
	</update>
</mapper>